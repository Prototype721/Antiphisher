{
  "name": "Anti-phisher",
  "description": "Это расширение служит для обнаружения мошеннических сайтов. Разработано Голиковым Михаилом Вячеславовичем под руководством Ануфриева В.Ю.",
  "version": "1.1",
  "icons": {
	"128": "128.png"
  },
  "permissions": ["tabs", "storage", "http://*/*", "https://*/*", "identity", "identity.email" ],
  "background": {
    "scripts": ["/js/fuse.js","background.js"]
  },

  "options_page": "options.html",

  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Anti-phisher!",
    "default_popup": "popup.html"
  },
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				  "default": "Ctrl+Shift+Y"
				}
		}
	},
  "manifest_version": 2
}
